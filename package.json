{
  "name": "nodeschool",
  "version": "1.0.0",
  "description": "Nodejs School trainings",
  "main": "index.js",
  "scripts": {
    "clear": "node -e \"process.stdout.write('\\033c')\"",
    "mocha": "mocha --compilers js:babel-register",
    "test": "mocha --compilers js:babel-register --recursive ./test/",
    "start": "nodemon --exec npx babel-node -- ./server/index.js",
    "utils": "npx babel-node -- ./utils/streams.js"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adolgoff/nodeschool.git"
  },
  "keywords": [
    "nodejs",
    "node",
    "training",
    "school",
    "npm"
  ],
  "author": "Alexander Dolgov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adolgoff/nodeschool/issues"
  },
  "homepage": "https://github.com/adolgoff/nodeschool#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "cookie": "^0.3.1",
    "crypto": "^1.0.1",
    "csvjson": "^5.0.0",
    "csvtojson": "^2.0.4",
    "express": "^4.16.3",
    "express-formidable": "^1.0.0",
    "hound": "^1.0.5",
    "jsonwebtoken": "^8.3.0",
    "passport": "^0.4.0",
    "replacestream": "^4.0.3"
  }
}
